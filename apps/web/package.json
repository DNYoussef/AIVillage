{
  "name": "aivillage-ui",
  "version": "2.0.0",
  "description": "AIVillage Distributed AI Platform - User Interface",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,css,md}\"",
    "start": "npm run dev",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "test:ui": "jest --testPathPattern=components",
    "test:hooks": "jest --testPathPattern=hooks",
    "test:utils": "jest --testPathPattern=utils",
    "dev:https": "vite --https",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@noble/ciphers": "^0.4.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "chart.js": "^4.2.1",
    "crypto-js": "^4.1.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.5.0",
    "simple-peer": "^9.11.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/simple-peer": "^9.11.5",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-jest": "^30.0.5",
    "core-js": "^3.45.1",
    "emittery": "^1.2.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.4.1",
    "typescript": "^4.9.5",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^1.2.1"
  },
  "keywords": [
    "aivillage",
    "distributed-ai",
    "fog-computing",
    "p2p-networking",
    "digital-twin",
    "blockchain",
    "compute-credits",
    "react",
    "typescript"
  ],
  "author": "AIVillage Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DNYoussef/AIVillage.git"
  },
  "bugs": {
    "url": "https://github.com/DNYoussef/AIVillage/issues"
  },
  "homepage": "https://aivillage.dev",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
