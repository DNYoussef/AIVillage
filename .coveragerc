[run]
source = src
omit =
    tests/*
    stubs/*
    models/*
    download_env/*
    experimental/*
    scripts/*
    .claude/*
    .cache/*
    tmp/*
    tmp_*/*
    **/__pycache__/*
    src/agent_forge/*
    src/software/*
    src/architecture/*
    src/servers/*
    src/services/*
    src/transport/*
    src/security/*
    src/monitoring/*
    src/ingestion/*
    src/android/*
    src/mcp_servers/*
    src/federation/*
    src/agents/*
    src/production/*
    src/core/*
    src/communications/*
    src/digital_twin/*
    src/governance/*
    src/hardware/*
    src/infrastructure/*
    src/hyperag/*
    src/migration/*
    src/nlp/*
    src/common/*
    src/cli/*
    src/compression/*
    src/deployment/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
